--gameobj system
function mk_go(x,y)
	return {x=x, y=y}
end

function mk_enemy(x,y,hp,label,tmr)
	local e=mk_go(x,y)
	e.hp=hp or 1
	e.t=0 or tmr
	e.label=label
	if(label=="boss")e.pfx=mk_bosspfx(e)
	add(enemies,e)
	return e
end

function mk_enemy_pol(sec,a,hp,label)
	sec*=30
	sec+=64
	return mk_enemy(64+sec*cos(a),
		64+sec*sin(a),hp,label)
end

function mk_bullet(
x,y,
angle,
src)
	local b=mk_go(x,y)
	b.angle=angle
	b.src=src
	add(bullets,b)
	return b
end

function set_pos(o,x,y)
	o.x,o.y=x,y
end
