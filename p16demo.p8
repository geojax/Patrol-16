--saving game, unfinished
function save_quit()
	dset(0,wave)
	dset(1,deaths)
	dset(2,start_time)
	dset(3,t)
	stop()
end

function load_game()
	wave=max(dget(0),1)
	deaths=dget(1) or 0
	start_time=dget(2)
	end_time=dget(3)
end

function new_game()
	for i=0,3 do
		dset(i,0)
	end
end
